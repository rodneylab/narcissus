{
  "name": "sveltekit-blog-mdx",
  "version": "1.0.0",
  "scripts": {
    "dev": "svelte-kit dev --port 3030",
    "build": "npm run generate:manifest && svelte-kit build && node ./generate-headers",
    "preview": "svelte-kit preview --port 3030",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate:images": "node ./generate-responsive-image-data.js",
    "generate:manifest": "node ./generate-manifest.js",
    "generate:sitemap": "node ./generate-sitemap.js",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "prepare": "husky install",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@fontsource/lato": "^4.5.0",
    "@fontsource/roboto-slab": "^4.5.1",
    "@fontsource/slabo-13px": "^4.5.0",
    "@fontsource/slabo-27px": "^4.5.0",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@rodneylab/svelte-social-icons": "^0.0.16",
    "@rodneylab/sveltekit-components": "^0.2.4",
    "@sveltejs/adapter-static": "^1.0.0-next.21",
    "@sveltejs/kit": "^1.0.0-next.192",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vanilla-extract/css": "^1.6.3",
    "@vanilla-extract/vite-plugin": "^3.0.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "dompurify": "^2.3.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "mdsvex": "^0.9.8",
    "object-hash": "^2.2.0",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "reading-time": "1.4.0",
    "sharp": "^0.29.2",
    "stylelint": "^14.0.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recommended-scss": "^5.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-scss": "^4.0.0",
    "svelte": "^3.44.0",
    "svelte-awesome": "^2.4.2",
    "svelte-check": "^2.2.8",
    "svelte-preprocess": "^4.9.8",
    "svelte-preprocess-import-assets": "^0.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vanilla-lazyload": "^17.5.0",
    "vite-imagetools": "^3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/narcissus-blog"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/narcissus-blog/issues"
  },
  "type": "module",
  "dependencies": {
    "node-html-parser": "^5.1.0"
  }
}
